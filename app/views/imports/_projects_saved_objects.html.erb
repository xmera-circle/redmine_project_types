<table id="saved-items" class="list">
  <thead>
  <tr>
    <th><%= t(:field_name) %></th>
    <th><%= t(:field_description) %></th>
    <th><%= t(:field_identifier) %></th>
    <th><%= t(:field_is_public) %></th>
    <th><%= t(:field_parent) %></th>
    <th><%= t(:field_inherit_members) %></th>
    <th><%= t(:field_project_type) %></th>
  </tr>
  </thead>
  <tbody>
  <% saved_objects.each do |project| %>
  <tr>
    <td><%= link_to project.name, project_path(project) %></td>
    <td><%= project.description %></td>
    <td><%= project.identifier %></td>
    <td><%= checked_image project.is_public %></td>
    <td><%= project.parent&.name %></td>
    <td><%= checked_image project.inherit_members %></td>
    <td><%= project.project_type.name %></td>
  </tr>
  <% end %>
  </tbody>
</table>